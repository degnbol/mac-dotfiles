{
    "console_double_click_select": true,
    "save_workspace": "never",
    "remove_history_duplicates": true,
    "num_spaces_for_tab": 4,
    "insert_matching": false,
    "margin_column": 100,
    "syntax_color_console": true,
    "scroll_past_end_of_document": true,
    "insert_spaces_around_equals": false,
    "default_encoding": "UTF-8",
    "code_completion": "manual",
    "code_completion_other": "manual",
    "editor_theme": "Pastel On Dark",
    "pdf_previewer": "none",
    "posix_terminal_shell": "zsh",
    "panes": {
        "quadrants": [
            "Source",
            "Console",
            "TabSet1",
            "TabSet2"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "VCS",
            "Tutorial",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Connections",
            "Packages",
            "Help",
            "Build",
            "Viewer"
        ],
        "console_left_on_top": false,
        "console_right_on_top": true
    },
    "busy_detection": "never",
    "terminal_bell_style": "none",
    "jobs_tab_visibility": "closed",
    "show_terminal_tab": false
}